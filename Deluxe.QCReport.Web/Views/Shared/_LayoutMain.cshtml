<body id="body-element">







    <div class="container-fluid">
        @RenderBody()
        @Html.Partial("_loader")
    </div>


   

    <!-- code added to highlight that you are in dev or stage -->
    <div id="environmentfooter" class="environmentfooter"></div>

    <script type="text/javascript">
       
        function setFooter() {
            el = document.getElementById('environmentfooter').style.background = 'rgba(224, 64, 96, 255)'
        }
       

        if (window.location.hostname.endsWith("qcdev.bydeluxe.com")) {
            environmentfooter.textContent = "Development"
            setFooter();
            
        }
        else if (window.location.hostname.endsWith("qctest.bydeluxe.com")) {
            environmentfooter.textContent = "UAT" //+ copyRight;
            setFooter();
        }
        else {
            environmentfooter.style = "visibility: hidden";
        }
    </script>


</body>
</html>
